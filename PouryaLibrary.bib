@article{Cannings,
annote = {The authors used MPI (Message Passing Interface) and a cluseter of 64 Computers with 100 Mbps Ethernet LAN to create a polygonizer farm. Cluster name is MACI.
        
The topology is a classic Master-Slave Architecture. Each node assigned a rank and two versions of Polygonization Algorithm provided. One for Masster duties and the other for slave duties.
        
Using Continuation method master will find all the cubes across the surface and save them in a list. Then using TCP/IP broadcast method available in MPI master node will send the entire list to all it's slave nodes.
        
Then Master will switch to slave and all the slaves will start triangulating the Voxels. After triangulation each slave node will return all the triangles to the Master node by using MPI::WORLD\_COMM::Allgatherv().
        
Master will put them in order and show.
        
Their method is not parallel in continuation part. And also communication between nodes is expensive. Finding cubes is the bottle neck of this architect.},
author = {Cannings, Richard and Wyvill, Brian},
title = {{PIP: The Parallel Implicit Polygonizer}}
}
