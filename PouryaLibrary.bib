issn = {0021-9290},
journal = {Journal of biomechanics},
keywords = {Anatomic,Brain,Brain Neoplasms,Brain Neoplasms: pathology,Brain Neoplasms: surgery,Brain: pathology,Brain: surgery,Computer Simulation,Computer-Assisted,Craniotomy,Elasticity,Finite Element Analysis,Humans,Image Interpretation,Imaging,Magnetic Resonance Imaging,Models,Neuronavigation,Nonlinear Dynamics,Phantoms},
month = jan,
number = {4},
